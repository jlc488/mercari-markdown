{
  "parser": "babel-eslint",
  "extends": ["airbnb"],
  "env": {
    "browser": true,
    "node": true
  },
  "rules": {
    "no-console": "off",
    "comma-dangle": "off",
    "react/jsx-filename-extension": "off",
    "linebreak-style": "off",
    "consistent-return": "off",
    "no-shadow": "off",
    "import/prefer-default-export": "off",
    "class-methods-use-this": "off",
    "object-curly-newline": "off"
  }
}